{
  "procedures": [
    {
      "name": "example-procedure",
      "requests": [
        {
          "tech": "http",
          "parameters": {
            "url": "https://vampi.tools.escape.tech",
            "method": "GET"
          },
          "extractions": [
            {
              "name": "example-extraction",
              "location": "body",
              "key": "message"
            }
          ]
        },
        {
          "tech": "http",
          "parameters": {
            "url": "https://vampi.tools.escape.tech",
            "method": "GET",
            "headers": [
              {
                "name": "X-Example-Header-Extracted",
                "values": ["{{ example-extraction }}"]
              }
            ],
            "body": null
          }
        }
      ]
    }
  ],
  "users": [
    {
      "name": "example-user",
      "credentials": {
        "username": null,
        "password": null,
        "headers": [
          {
            "name": "X-Example-Header",
            "values": ["example-value"]
          }
        ],
        "body": null
      },
      "authentication": {
        "procedure": "example-procedure",
        "injections": [
          {
            "location": "header",
            "key": "X-Injected-Header",
            "prefix": "Prefixed ",
            "variable": "example-extraction"
          }
        ]
      },
      "refresh": null
    }
  ],
  "$schema": "./multiauth-schema.json"
