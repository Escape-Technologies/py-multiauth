{
  "version": "0.3.0",
  "configurations": [
    {
      "name": "Generate docs",
      "type": "python",
      "request": "launch",
      "program": "scripts/generate_docs.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Validate local config",
      "type": "python",
      "request": "launch",
      "module": "multiauth.cli.cli",
      "args": [
        "-f",
        ".multiauthrc.json",
        "validate",
        "-u",
        "user",
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}